@model IEnumerable<SOAC_RKU.Models.Clubs>
@{
    ViewData["Title"] = "Clubdetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div style="margin: 150px">
    <table class="table border-2">
        <thead>
            <tr>
                <th>Enrollment Id</th>
                <th>Club Name</th>
                <th>Mentor Name</th>
                <th>Fees</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Clubs items in Model)
            {
                if (items.Enrollment_id == Context.Session.GetString("EnrollmentId"))
                {

                    <tr>

                        <td>@items.Enrollment_id</td>
                        <td>@items.Club_name</td>
                        <td>@items.Mentor_name</td>
                        <td>@items.Fees</td>
                        <td>
                            <a   class="btn btn-warning">Delete</a>
                        </td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>

